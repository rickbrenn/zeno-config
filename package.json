{
	"name": "zeno-config",
	"version": "2.0.0",
	"description": "Preconfigured and opinionated ESLint, Prettier, and TypeScript setup",
	"author": "Rick Brenn <brenn.rick@gmail.com>",
	"license": "MIT",
	"keywords": [
		"eslint",
		"prettier",
		"typescript",
		"config",
		"linting",
		"code-style",
		"react",
		"eslint-config"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rickbrenn/zeno-config.git"
	},
	"homepage": "https://github.com/rickbrenn/zeno-config#readme",
	"bugs": {
		"url": "https://github.com/rickbrenn/zeno-config/issues"
	},
	"type": "module",
	"packageManager": "pnpm@10.25.0",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"inspect": "eslint --inspect-config",
		"lint": "eslint \"**/*.{js,cjs,mjs}\""
	},
	"exports": {
		"./eslint": {
			"types": "./src/eslint/index.d.ts",
			"default": "./src/eslint/index.js"
		},
		"./prettier": {
			"types": "./src/prettier.d.ts",
			"default": "./src/prettier.js"
		},
		"./tsconfig": "./src/tsconfig.base.json",
		"./tsconfig-react": "./src/tsconfig.react.json",
		"./extensions": {
			"types": "./src/extensions.d.ts",
			"default": "./src/extensions.js"
		}
	},
	"files": [
		"src/**"
	],
	"peerDependencies": {
		"eslint": "^9.39.2",
		"prettier": "^3.8.0",
		"typescript": "^5.9.3"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"devDependencies": {
		"eslint": "9.39.2",
		"prettier": "3.8.0",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@eslint/js": "9.39.2",
		"@stylistic/eslint-plugin": "5.7.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-n": "17.23.2",
		"eslint-plugin-prettier": "5.5.5",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "7.0.1",
		"eslint-plugin-react-you-might-not-need-an-effect": "0.8.5",
		"eslint-plugin-unicorn": "62.0.0",
		"globals": "17.0.0",
		"typescript-eslint": "8.53.1"
	}
}

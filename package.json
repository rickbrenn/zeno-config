{
	"name": "zeno-config",
	"version": "1.0.0",
	"description": "Preconfigured and opinionated ESLint, Prettier, and TypeScript setup",
	"author": "Rick Brenn <brenn.rick@gmail.com>",
	"license": "MIT",
	"keywords": [
		"eslint",
		"prettier",
		"typescript",
		"config",
		"linting",
		"code-style",
		"react",
		"eslint-config"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rickbrenn/zeno-config.git"
	},
	"homepage": "https://github.com/rickbrenn/zeno-config#readme",
	"bugs": {
		"url": "https://github.com/rickbrenn/zeno-config/issues"
	},
	"type": "module",
	"packageManager": "pnpm@10.25.0",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"inspect": "pnpm dlx eslint --inspect-config"
	},
	"exports": {
		"./eslint": {
			"types": "./src/eslint/index.d.ts",
			"default": "./src/eslint/index.js"
		},
		"./prettier": {
			"types": "./src/prettier.d.ts",
			"default": "./src/prettier.js"
		},
		"./tsconfig": "./src/tsconfig.base.json",
		"./tsconfig-react": "./src/tsconfig.react.json",
		"./extensions": {
			"types": "./src/extensions.d.ts",
			"default": "./src/extensions.js"
		}
	},
	"files": [
		"src/**"
	],
	"peerDependencies": {
		"@eslint/js": "^9.39.2",
		"@stylistic/eslint-plugin": "^5.6.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-n": "^17.23.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-you-might-not-need-an-effect": "^0.7.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-jsx-a11y": {
			"optional": true
		},
		"eslint-plugin-react": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-react-you-might-not-need-an-effect": {
			"optional": true
		},
		"typescript": {
			"optional": true
		},
		"typescript-eslint": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/js": "9.39.2",
		"@stylistic/eslint-plugin": "5.6.1",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-n": "17.23.1",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "7.0.1",
		"eslint-plugin-react-you-might-not-need-an-effect": "0.7.0",
		"eslint-plugin-unicorn": "62.0.0",
		"prettier": "3.7.4",
		"typescript": "5.9.3",
		"typescript-eslint": "8.50.1"
	},
	"dependencies": {
		"globals": "16.5.0"
	}
}
